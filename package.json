{
  "name": "@noveum/trace",
  "version": "0.1.0",
  "description": "TypeScript SDK for tracing LLM, RAG, and agent applications with Noveum",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./integrations/express": {
      "types": "./dist/integrations/express.d.ts",
      "import": "./dist/integrations/express.mjs",
      "require": "./dist/integrations/express.js"
    },
    "./integrations/nextjs": {
      "types": "./dist/integrations/nextjs.d.ts",
      "import": "./dist/integrations/nextjs.mjs",
      "require": "./dist/integrations/nextjs.js"
    },
    "./integrations/hono": {
      "types": "./dist/integrations/hono.d.ts",
      "import": "./dist/integrations/hono.mjs",
      "require": "./dist/integrations/hono.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "changeset publish",
    "version": "changeset version"
  },
  "keywords": [
    "tracing",
    "observability",
    "llm",
    "ai",
    "monitoring",
    "typescript",
    "nodejs",
    "nextjs",
    "hono",
    "express"
  ],
  "author": "Noveum Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noveum/noveum-trace-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/Noveum/noveum-trace-typescript/issues"
  },
  "homepage": "https://github.com/Noveum/noveum-trace-typescript#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}

